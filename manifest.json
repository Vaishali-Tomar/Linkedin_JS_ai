
{
  "manifest_version": 3,
  "name": "LinkedIn GPT",
  "version": "1.0",
  "description": "Extend posibilities of LinkedIn by using ChatGPT",
  "icons": {
    "16":"icons/icon-16.png",
        "32":"icons/icon-32.png",
        "48":"icons/icon-48.png",
        "128":"icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": ["activeTab", "scripting"]
}
